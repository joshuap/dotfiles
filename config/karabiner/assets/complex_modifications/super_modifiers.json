{
  "title": "Super modifiers",
  "rules": [
    {
      "description": "Send super (left alt) when tab is held.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab"
          },
          "to": [
            {
              "key_code": "left_alt"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "tab"
            }
          ]
        }
      ]
    },
    {
      "description": "Send super (left alt) when backslash is held.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "backslash"
          },
          "to": [
            {
              "key_code": "left_alt"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "backslash"
            }
          ]
        }
      ]
    }
  ]
}
